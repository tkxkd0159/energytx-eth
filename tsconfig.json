{
    "display": "Node 14",
    "compilerOptions": {
        "downlevelIteration": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "lib": ["es2020"],
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist",
        "removeComments": true,
        "sourceMap": false,
        "strict": true,
        "skipLibCheck": true,
        "target": "es2020"
    }
}